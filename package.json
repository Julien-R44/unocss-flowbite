{
  "name": "@julr/unocss-preset-flowbite-monorepo",
  "version": "0.0.1-beta.0",
  "packageManager": "pnpm@7.5.0",
  "description": "ðŸ’… An adaptation of the Flowbite Tailwind plugin for UnoCSS",
  "keywords": [
    "unocss",
    "unocss-preset",
    "flowbite",
    "headless"
  ],
  "license": "MIT",
  "author": "Julien Ripouteau <julien@ripouteau.com>",
  "scripts": {
    "build": "rimraf dist && unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepublishOnly": "pnpm build",
    "release": "bumpp --commit --push --tag && pnpm publish"
  },
  "devDependencies": {
    "@julr/eslint-config": "^0.3.0",
    "@types/node": "^18.0.0",
    "bumpp": "^8.2.1",
    "eslint": "^8.19.0",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.7.4",
    "unbuild": "^0.7.4",
    "unocss": "^0.43.0",
    "vite": "^2.9.13"
  },
  "prettier": {
    "trailingComma": "es5",
    "semi": false,
    "singleQuote": true,
    "useTabs": false,
    "quoteProps": "consistent",
    "bracketSpacing": true,
    "arrowParens": "always",
    "printWidth": 100
  },
  "eslintConfig": {
    "extends": "@julr",
    "rules": {
      "n/no-unsupported-features/es-syntax": "off",
      "n/no-missing-import": "off",
      "n/no-unpublished-import": "off"
    }
  }
}
