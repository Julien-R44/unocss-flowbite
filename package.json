{
  "name": "@julr/unocss-preset-flowbite",
  "version": "0.0.0",
  "description": "ðŸ’… An adaptation of the Flowbite Tailwind plugin for UnoCSS",
  "keywords": [
    "unocss",
    "unocss-preset",
    "flowbite",
    "headless"
  ],
  "homepage": "https://github.com/action-hong/unocss-preset-scrollbar#readme",
  "bugs": {
    "url": "https://github.com/action-hong/unocss-preset-scrollbar/issues"
  },
  "license": "MIT",
  "author": "Julien Ripouteau <julien@ripouteau.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/action-hong/unocss-preset-scrollbar.git"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rimraf dist && unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepublishOnly": "pnpm build",
    "release": "bumpp --commit --push --tag && pnpm publish"
  },
  "devDependencies": {
    "@julr/eslint-config": "^0.3.0",
    "@types/node": "^18.0.0",
    "eslint": "^8.19.0",
    "mini-svg-data-uri": "^1.4.4",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.7.4",
    "unbuild": "^0.7.4",
    "unocss": "^0.43.0",
    "vite": "^2.9.13"
  },
  "prettier": {
    "trailingComma": "es5",
    "semi": false,
    "singleQuote": true,
    "useTabs": false,
    "quoteProps": "consistent",
    "bracketSpacing": true,
    "arrowParens": "always",
    "printWidth": 100
  },
  "eslintConfig": {
    "extends": "@julr",
    "rules": {
      "n/no-unsupported-features/es-syntax": "off"
    }
  }
}
