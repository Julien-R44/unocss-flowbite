{
  "name": "@julr/unocss-preset-flowbite",
  "version": "0.0.1-beta.1",
  "packageManager": "pnpm@7.5.0",
  "description": "ðŸ’… An adaptation of the Flowbite Tailwind plugin for UnoCSS",
  "author": "Julien Ripouteau <julien@ripouteau.com>",
  "license": "MIT",
  "homepage": "https://github.com/Julien-R44/unocss-preset-flowbite#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Julien-R44/unocss-preset-flowbite.git"
  },
  "bugs": {
    "url": "https://github.com/Julien-R44/unocss-preset-flowbite/issues"
  },
  "keywords": [
    "unocss",
    "unocss-preset",
    "flowbite",
    "headless"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "rimraf dist && unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepublishOnly": "pnpm build",
    "release": "bumpp --commit --push --tag && pnpm publish"
  },
  "peerDependencies": {
    "unocss": ">= 0.31.13 < 1"
  },
  "devDependencies": {
    "@julr/eslint-config": "^0.3.2",
    "@types/node": "^18.7.14",
    "bumpp": "^8.2.1",
    "eslint": "^8.23.0",
    "mini-svg-data-uri": "^1.4.4",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.8.2",
    "unbuild": "^0.8.10",
    "unocss": "^0.45.14",
    "vite": "^3.0.9"
  },
  "prettier": {
    "trailingComma": "es5",
    "semi": false,
    "singleQuote": true,
    "useTabs": false,
    "quoteProps": "consistent",
    "bracketSpacing": true,
    "arrowParens": "always",
    "printWidth": 100
  },
  "eslintConfig": {
    "extends": "@julr",
    "rules": {
      "n/no-unsupported-features/es-syntax": "off",
      "n/no-missing-import": "off"
    }
  }
}
